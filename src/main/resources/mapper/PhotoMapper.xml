<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.glow.banana.mapper.PhotoMapper">
    <insert id="insertPhoto" parameterType="com.glow.banana.domain.Photo">
        INSERT INTO photos (file_name, file_type, file_size)
        VALUES (#{fileName}, #{fileType}, #{fileSize}
               )



    </insert>


</mapper>
